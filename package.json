{
  "name": "rollup-plugin-npm",
  "description": "Bundle third-party dependencies in node_modules",
  "version": "1.3.0",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.0.0",
    "babel-register": "^6.3.13",
    "es5-ext": "^0.10.11",
    "eslint": "^1.7.3",
    "mocha": "^2.3.3",
    "rollup": "^0.22.0",
    "rollup-plugin-babel": "^2.2.0",
    "rollup-plugin-commonjs": "^2.0.0",
    "string-capitalize": "^1.0.1",
    "vlq": "^0.2.1"
  },
  "main": "dist/rollup-plugin-npm.cjs.js",
  "jsnext:main": "dist/rollup-plugin-npm.es6.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "pretest": "npm run build",
    "build": "rollup -c -f cjs -o dist/rollup-plugin-npm.cjs.js && rollup -c -f es6 -o dist/rollup-plugin-npm.es6.js",
    "prepublish": "npm test"
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "dependencies": {
    "browser-resolve": "^1.11.0",
    "builtin-modules": "^1.1.0",
    "resolve": "^1.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollup/rollup-plugin-npm.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "es2015",
    "npm",
    "modules"
  ],
  "author": "Rich Harris <richard.a.harris@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rollup/rollup-plugin-npm/issues"
  },
  "homepage": "https://github.com/rollup/rollup-plugin-npm#readme"
}
